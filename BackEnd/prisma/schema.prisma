generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id             Int    @id @default(autoincrement())
  nome           String
  sobrenome      String
  telefone       String
  ra             Int    @unique
  senha          String
  confirmarSenha String

  @@map("users")
}
